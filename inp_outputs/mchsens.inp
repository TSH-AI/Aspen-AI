DYNAMICS
    DYNAMICS RESULTS=ON

TITLE 'Methylcyclohexane Recovery Process' 

IN-UNITS ENG SHORT-LENGTH=in 

DEF-STREAMS CONVEN ALL 

DIAGNOSTICS 
    TERMINAL SIM-LEVEL=4 CONV-LEVEL=4 COST-LEVEL=4 PROP-LEVEL=4  &
        ECON-LEVEL=4 STREAM-LEVEL=4 SYS-LEVEL=4 

MODEL-OPTION 

DESCRIPTION "
    This example is used in chapter 3 of 'Aspen Plus Getting Started - 
    Building and Running A Process Model'. 
    "

DATABANKS 'APV140 PURE40' / 'APV140 AQUEOUS' / 'APV140 SOLIDS' &
         / 'APV140 INORGANIC' / NOASPENPCD

PROP-SOURCES 'APV140 PURE40' / 'APV140 AQUEOUS' /  &
        'APV140 SOLIDS' / 'APV140 INORGANIC'

COMPONENTS 
    TOLUENE C7H8 / 
    PHENOL C6H6O / 
    MCH C7H14-6 

SOLVE 
    RUN-MODE MODE=SIM 

FLOWSHEET 
    BLOCK B1 IN=1 2 OUT=3 4 

PROPERTIES UNIFAC 

PROP-SET TXPORT RHOMX MUMX SIGMAMX UNITS='lb/cuft'  &
        SUBSTREAM=MIXED PHASE=V L 
;  'Density, viscosity, and surface tension' 
    

STREAM 1 
    SUBSTREAM MIXED TEMP=220 PRES=20 
    MOLE-FLOW TOLUENE 200 / MCH 200 

STREAM 2 
    SUBSTREAM MIXED TEMP=220 PRES=20 
    MOLE-FLOW PHENOL 1200 

BLOCK B1 RADFRAC 
    PARAM NSTAGE=22 ALGORITHM=STANDARD MAXOL=25 DAMPING=NONE 
    PARAM2 
    COL-CONFIG CONDENSER=TOTAL 
    FEEDS 1 14 / 2 7 
    PRODUCTS 3 1 L / 4 22 L 
    P-SPEC 1 16 / 22 20.2 
    COL-SPECS MOLE-D=200 MOLE-RR=8 

EO-CONV-OPTI 

SENSITIVITY S-1 
    DEFINE XMCH MOLE-FRAC STREAM=3 SUBSTREAM=MIXED  &
        COMPONENT=MCH 
    DEFINE QCOND BLOCK-VAR BLOCK=B1 VARIABLE=COND-DUTY  &
        SENTENCE=RESULTS UOM="Btu/hr" 
    DEFINE QREB BLOCK-VAR BLOCK=B1 VARIABLE=REB-DUTY  &
        SENTENCE=RESULTS UOM="Btu/hr" 
    TABULATE 1 "XMCH" COL-LABEL="MCH" "PURITY" "IN DIST" 
    TABULATE 2 "QCOND" COL-LABEL="CONDENS" "DUTY" 
    TABULATE 3 "QREB" COL-LABEL="REBOILER" "DUTY" 
    VARY STREAM-VAR STREAM=2 SUBSTREAM=MIXED VARIABLE=MOLE-FLOW  &
        UOM="lbmol/hr" LABEL="PHENOL" "FLOWRATE" 
    RANGE OPT-LIST=RANGE LOWER="1200" UPPER="2000"  &
        INCR="100.000000" 

STREAM-REPOR MOLEFLOW MOLEFRAC PROPERTIES=TXPORT 
